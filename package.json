{
  "name": "inplayer-ui-components",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "lerna run clean --stream --parallel",
    "build": "lerna run build --stream --parallel",
    "lint": "eslint . --ext .js,.ts,.tsx -f codeframe",
    "pretest": "yarn typecheck && yarn lint",
    "test": "yarn test:unit",
    "test:unit": "mocha \"packages/**/test/**/*.spec.ts?(x)\"",
    "start": "yarn workspace @sample/app start",
    "typecheck": "tsc --noEmit",
    "prettify": "npx prettier \"packages/**/*.{js,ts,tsx}\" --write"
  },
  "dependencies": {
    "@inplayer-org/paywall-templates": "file:packages/paywall-templates",
    "undefined": "file:packages/inplayer-ui"
  },
  "devDependencies": {
    "@types/styled-components": "^4.4.1",
    "eslint-watch": "^6.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "lerna": "^3.20.2",
    "polished": "^3.4.2",
    "prettier": "^1.19.1",
    "prettierrc": "^0.0.0-5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1",
    "styled-components-modifiers": "^1.2.5",
    "styled-tools": "^1.7.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
